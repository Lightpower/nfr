<% if current_user.present? && current_user.team.present? %>
  <%= form_for :code_string, url: m_path, html: {class: 'button_to'} do |f| %>
    <%= f.label :code, 'Введите код:' %><br>
    <%= f.text_field :code %><br>
    <%= f.submit 'OK' %><br>
    <%= link_to 'Полная версия', root_path %>
  <% end %>
<% end %>
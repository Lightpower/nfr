<% if zones.present? || @game.tasks.unzoned.present? %>
  <% zones.each do |zone|  %>
    <%=  render 'game_strategies/conquest/zones/item', zone: zone %>
	<% end %>
  <%=  render('game_strategies/conquest/zones/item', zone: nil) if @game.tasks.unzoned.present? %>
<% else %>
	 <div class='zone'>Нет доступных зон</div>
<% end %>